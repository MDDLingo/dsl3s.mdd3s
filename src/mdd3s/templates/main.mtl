[import mdd3s::templates::animatPortrayal/]
[import mdd3s::templates::Spatial/]
[import mdd3s::templates::SpatialPortrayal/]

[template public mainClass(aClass : Class)]
	
	[comment @main /]
	[aClass.sim()/]
	[aClass.sim_gui()/]
	[aClass.factory()/]
